@using EntityLayer.Concrete
@model List<Contact>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Bikes</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Address</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Status</th>
        <th>Update</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ContactId</td>
            <td>@item.Address</td>
            <td>@item.Phone</td>
            <td>@item.Email</td>
            <td>@item.Status</td>
            <td><a href="/Admin/Contact/UpdateContact/@item.ContactId" class="btn btn-outline-success">Update</a></td>
        </tr>
    }
</table>
